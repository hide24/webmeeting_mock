<p id="notice"><%= notice %></p>

<dl class="row">
  <dt class="col-sm-2">Display name:</dt>
  <dd class="col-sm-10"><%= @user.display_name %></dd>

  <dt class="col-sm-2">Given name:</dt>
  <dd class="col-sm-10"><%= @user.given_name %></dd>

  <dt class="col-sm-2">Surname:</dt>
  <dd class="col-sm-10"><%= @user.surname %></dd>

  <dt class="col-sm-2">Mail:</dt>
  <dd class="col-sm-10"><%= @user.mail %></dd>

  <dt class="col-sm-2">Guest:</dt>
  <dd class="col-sm-10"><%= @user.guest %></dd>

  <dt class="col-sm-2">Another Mail:</dt>
  <dd class="col-sm-10"><%= @user.another_mail %></dd>
</dl>

<%= link_to 'Edit', edit_user_path(@user), class: "btn btn-primary" %> 
<%= link_to 'Back', users_path, class: "btn btn-outline-secondary" %>
